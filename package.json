{
  "name": "toodoo-frontend-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "preview": "vite preview",
    "format": "prettier --write .",
    "sta": "sta -p https://not-woowacourse-api.yopark.dev/api-docs-json -o __generated__ --modular --axios --extract-request-params --extract-request-body --extract-response-body --extract-response-error",
    "update-deps": "ncu -u && pnpm i",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.1.0",
    "@emotion/react": "^11.13.3",
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-query-devtools": "^5.59.20",
    "date-fns": "^4.1.0",
    "es-hangul": "^2.1.0",
    "es-toolkit": "^1.26.1",
    "ky": "^1.7.2",
    "next-themes": "^0.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.27.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@tanstack/eslint-plugin-query": "^5.59.7",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "swagger-typescript-api": "^13.0.22",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^5.1.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
